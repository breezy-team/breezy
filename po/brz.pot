msgstr ""

#: breezy/builtins.py:3535
msgid "    Ignore files whose name begins with the \"#\" character::"
msgstr ""

#: breezy/builtins.py:3537
msgid "        brz ignore \"RE:^#\""
msgstr ""

#: breezy/builtins.py:3539 breezy/builtins.py:3543
msgid "    Ignore .o files under the lib directory::"
msgstr ""

#: breezy/builtins.py:3541
msgid "        brz ignore \"lib/**/*.o\""
msgstr ""

#: breezy/builtins.py:3545
msgid "        brz ignore \"RE:lib/.*\\.o\""
msgstr ""

#: breezy/builtins.py:3547
msgid "    Ignore everything but the \"debian\" toplevel directory::"
msgstr ""

#: breezy/builtins.py:3549
msgid "        brz ignore \"RE:(?!debian/).*\""
msgstr ""

#: breezy/builtins.py:3551
msgid ""
"    Ignore everything except the \"local\" toplevel directory,\n"
